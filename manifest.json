{
  "manifest_version": 3,
  "name": "EoH Guard",
  "description": "Extension for blocking phishing sites and protecting your browsing.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "icons": {
    "128": "icons/128.png"
  },
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "activeTab",
        "scripting",
        "notifications"
    ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
